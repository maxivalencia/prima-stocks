{% extends 'base.html.twig' %}

{% block title %}Access{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <h1>Access</h1>
    </div>
    <hr>

    <table class="table">
        <tbody>
            <tr>
                <td>Id</td>
                <td>{{ access.id }}</td>
            </tr>
            <tr>
                <td>Access</td>
                <td>{{ access.access }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('access_index') }}" class="btn btn-primary">Retour</a>
    <br><br>
    <a href="{{ path('access_edit', {'id': access.id}) }}" class="btn btn-warning">Edition</a>
    <br><br>
    {{ include('access/_delete_form.html.twig') }}
</div>
{% endblock %}
