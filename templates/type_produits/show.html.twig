{% extends 'base.html.twig' %}

{% block title %}Type Produit{% endblock %}

{% block body %}
<div class="container">
    <div class="row">
        <h1>Type de Produit</h1>
    </div>
    <hr>

    <table class="table">
        <tbody>
            <tr>
                <td>Id</td>
                <td>{{ type_produit.id }}</td>
            </tr>
            <tr>
                <td>Type</td>
                <td>{{ type_produit.type }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('type_produits_index') }}" class="btn btn-primary">Retour</a>
    <br><br>
    <a href="{{ path('type_produits_edit', {'id': type_produit.id}) }}" class="btn btn-warning">Edition</a>
    <br><br>
    {{ include('type_produits/_delete_form.html.twig') }}
</div>
{% endblock %}
